<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Insight Web Administration Application</display-name>

    <servlet>
        <servlet-name>RootServlet</servlet-name>
        <display-name>Insight Web Administration Application</display-name>
        <description>The index page for the Insight web-administration application</description>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>RootServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>
